@model List<Hospital_Management.Models.Dto.UserDto>

@{
}

<h1>Users</h1>

<table class="table">
	<thead>
		<tr>
			<th scope="col">Username</th>
			<th scope="col">User Type</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var user in Model)
		{
			<tr>
				<td>@user.Username</td>
				<td>@user.UserType</td>
				<td>
					<a href="Users/Details/@user.UserId">Details</a>
					<a href="Users/Delete/@user.UserId">Delete</a>
				</td>
			</tr>
		}
	</tbody>
</table>
